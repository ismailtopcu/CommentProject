@model List<Title>
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class=" container">
    <table class=" table table-bordered">
        <tr>
            <th>#</th>
            <th>Başlık</th>
            <th>Yorumlar</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.TitleID</td>
                <td>@item.TitleName</td>
                <td><a href="#" class="btn btn-outline-primary">Yorumları Görüntüle</a></td>
            </tr>
        }
    </table>
</div>
